{
    "name": "horde/dev.horde.org",
    "description": "The horde.org repositories for Horde 6 Framework",
    "homepage": "https://dev.horde.org",
    "output-dir": ".",
    "output-html": true,
    "providers": true,
    "archive": {
        "directory": "dist",
        "format": "tar",
        "skip-dev": false,
        "checksum": true,
        "ignore-filters": false,
        "override-dist-type": true,
        "rearchive": true
    },
    "twig-template": "views/satis.html.twig",
    "require-all": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ralflang/Autoloader",
            "name": "horde/autoloader"
        }
    ]
}
